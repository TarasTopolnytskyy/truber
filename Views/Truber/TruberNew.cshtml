@model Truber

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="~/css/style.css"/>
</head>
<body>
    <nav class="navbar navbar-dark bg-dark">
        <h1 class="wht"><img src="/../img/logo.jpg" width="80" height="80" class="d-inline-block align-top" alt=""> Truber</h1>
        
            <p class="wht">
                <a asp-action="Index" asp-controller="Truber" class="btn btn-success">Home</a>
                <a asp-action="LogOut" asp-controller="Home" class="btn btn-outline-danger">LogOut</a>
            </p>
    </nav>
    <div class="accesBoxesDriver">
        <form class="col-11" asp-action="Create" asp-controller="Truber" method="post">
            <div class="form-group">
                <span asp-validation-for="AddressPickUp"></span>
                <label asp-for="AddressPickUp"></label>
                <input asp-for="AddressPickUp" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="PickUpTime"></span>
                <label asp-for="PickUpTime"></label>
                <input asp-for="PickUpTime" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="AddressDropOff"></span>
                <label asp-for="AddressDropOff"></label>
                <input asp-for="AddressDropOff" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="DropOffTime"></span>
                <label asp-for="DropOffTime"></label>
                <input asp-for="DropOffTime" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="Weight"></span>
                <label asp-for="Weight"></label>
                <input asp-for="Weight" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="Height"></span>
                <label asp-for="Height"></label>
                <input asp-for="Height" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="Width"></span>
                <label asp-for="Width"></label>
                <input asp-for="Width" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="Length"></span>
                <label asp-for="Length"></label>
                <input asp-for="Length" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="TotalAmoutOfMiles"></span>
                <label asp-for="TotalAmoutOfMiles"></label>
                <input asp-for="TotalAmoutOfMiles" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="PhoneNumber"></span>
                <label asp-for="PhoneNumber"></label>
                <input asp-for="PhoneNumber" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="Price"></span>
                <label asp-for="Price"></label>
                <input asp-for="Price" class="form-control">
            </div>
    
            <div class="form-group">
                <input type="submit" class="btn btn-outline-primary" value="Add Order">
            </div>

        </form>
    </div>
</body>
</html>
